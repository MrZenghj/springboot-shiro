			<groupId>com.emc.ecs</groupId>
			<artifactId>object-transform</artifactId>
			<version>1.1.0</version>
		</dependency>

		<dependency>
			<groupId>com.emc.ecs</groupId>
			<artifactId>smart-client</artifactId>
			<version>2.2.1</version>
		</dependency>

		<dependency>
			<groupId>org.glassfish.jersey.core</groupId>
